{
  "name": "ableton-kit-maker",
  "description": "Build Ableton Drum Rack from your NI Extensions",
  "author": "badazzdev",
  "version": "0.0.1",
  "main": "./public/electron.js",
  "productName": "Ableton Kits",
  "homepage": "./",
  "build": {
    "appId": "<com.your_app>",
    "extends": null,
    "files": [
      "./public/electron.js",
      "./build/**/*",
      "./utils/**/*",
      "./controller/**/*",
      "./mains/**/*",
      "./cache/**/*"
    ]
  },
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run start-react\" \"wait-on http://localhost:3000 && npm run start-electron\"",
    "start-react": "export BROWSER=none && react-scripts start",
    "start-electron": "export ELECTRON_START_URL=http://localhost:3000 && nodemon --watch . --exec \"electron . --debug\"",
    "build": "npm run build-react && npm run build-electron",
    "build-react": "react-scripts build",
    "build-electron": "electron-builder",
    "test": "eslint \"**/*.js\""
  },
  "dependencies": {
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "ejs": "3.1.3",
    "electron-devtools-installer": "3.1.1",
    "electron-is-dev": "1.2.0",
    "fs-extra": "9.0.1",
    "prop-types": "15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1"
  },
  "devDependencies": {
    "concurrently": "5.2.0",
    "electron": "9.1.0",
    "electron-builder": "22.7.0",
    "eslint": "7.5.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.3",
    "husky": "4.2.5",
    "nodemon": "2.0.4",
    "prettier": "2.0.5",
    "wait-on": "5.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test",
      "pre-push": "npm run test"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
